{
  "env": {
    "mocha": true
  },
  "extends": [
    "ericgio",
    "plugin:jsx-a11y/recommended"
  ],
  "globals": {},
  "parser": "babel-eslint",
  "plugins": [
    "jsx-a11y",
  ],
  "rules": {
    "arrow-parens": [2, "always"],
    "no-control-regex": [0],
    "sort-keys": [2, "asc", {"caseSensitive": false, "natural": true}],
  }
}
