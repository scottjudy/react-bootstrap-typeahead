{
  "plugins": [
    "dev-expression",
    ["transform-imports", {
      "lodash": {
        "transform": "lodash/${member}",
        "preventFullImport": true
      },
      "prop-types-extra": {
        "transform": "prop-types-extra/lib/${member}",
        "preventFullImport": true
      },
      "react-bootstrap": {
        "transform": "react-bootstrap/lib/${member}",
        "preventFullImport": true
      },
      "react-overlays": {
        "transform": "react-overlays/lib/${member}",
        "preventFullImport": true
      }
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
      ],
    }
  },
  "presets": [
    "env",
    "react",
    "stage-1",
  ],
}
